<body>
  <% if(effectData !== undefined && (effectData.name === "Snow")){ %>
  <div class="effect <%= effectData.className %>">
    <div class="snow__item"></div>
    <div class="snow__item"></div>
    <div class="snow__item"></div>
    <div class="snow__item"></div>
    <div class="snow__item"></div>
    <div class="snow__item"></div>
    <div class="snow__item"></div>
    <div class="snow__item"></div>
    <div class="snow__item"></div>
  <% } else { %>
    <% if(effectData !== undefined && effectData.name === "Leaves"){ %>
    <div class="effect" id="leaves">
      <script type="text/javascript">
        var falling = true;

        var total = 30;
        var container = document.getElementById("leaves"),	w = window.innerWidth , h = window.innerHeight;

        for (i=0; i<total; i++){
         var Div = document.createElement('div');
         TweenLite.set(Div,{attr:{class:'dot'},x:R(0,w),y:R(-200,-150),z:R(-200,200)});
         container.appendChild(Div);
         animm(Div);
        }

        function animm(elm){
         TweenMax.to(elm,R(6,15),{y:h+100,ease:Linear.easeNone,repeat:-1,delay:-15});
         TweenMax.to(elm,R(4,8),{x:'+=100',rotationZ:R(0,180),repeat:-1,yoyo:true,ease:Sine.easeInOut});
         TweenMax.to(elm,R(2,8),{rotationX:R(0,360),rotationY:R(0,360),repeat:-1,yoyo:true,ease:Sine.easeInOut,delay:-5});
        };

        function R(min,max) {return min+Math.random()*(max-min)};
      </script>
    <% } else { %>
    <div class="effect">
    <% } %>
  <% } %>

    <div class="spn_hol">
      <div class="spinner">
        <div class="bounce1"></div>
        <div class="bounce2"></div>
        <div class="bounce3"></div>
