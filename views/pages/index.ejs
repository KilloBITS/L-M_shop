            <div class="slide__bg min"
              style="background-image: url(../../../data/slider/min/<%= slides[i].slideImage %>)"></div>

            <% if(slides[i].voting){ %>
            <div class="slide_poll">
              <% for(var iv = 0; iv < slides[i].options.length; iv++) { %>
              <div class="poll_option" onclick="clickPoll(<%= iv %>)">
                <div class="poll_select"><i class="fa fa-circle-o" aria-hidden="true"></i>
                  <%= slides[i].options[iv] %></div>
              </div>
              <% } %>
              <script type="text/javascript">
                var clickPoll = function (a) {
                  $.post('/setPoll', { a: a }, function (res) {
                    console.log(res);
                  });
                }
              </script>
            </div>
            <% } %>
            <% if(slides[i].isBtn){ %>
            <a href="<%= slides[i].slideLink %>" target="_blank">
              <div class="slide-button">
                <%= slides[i].slideName[numLang] %>
              </div>
            </a>
